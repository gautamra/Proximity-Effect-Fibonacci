@article{halterman_proximity_2001,
	title = {Proximity effects at ferromagnet-superconductor interfaces},
	volume = {65},
	url = {https://link.aps.org/doi/10.1103/PhysRevB.65.014509},
	doi = {10.1103/PhysRevB.65.014509},
	abstract = {We study proximity effects at ferromagnet-superconductor interfaces by self-consistent numerical solution of the Bogoliubov{\textendash}de Gennes equations for the continuum, without any approximations. Our procedures allow us to study systems with long superconducting coherence lengths. We obtain results for the pair potential, the pair amplitude, and the local density of states. We use these results to extract the relevant proximity lengths. We find that the superconducting correlations in the ferromagnet exhibit a damped oscillatory behavior that is reflected in both the pair amplitude and the local density of states. The characteristic length scale of these oscillations is approximately inversely proportional to the exchange field, and is independent of the superconducting coherence length in the range studied. We find the superconducting coherence length to be nearly independent of the ferromagnetic polarization.},
	number = {1},
	urldate = {2020-09-29},
	journal = {Physical Review B},
	author = {Halterman, Klaus and Valls, Oriol T.},
	month = nov,
	year = {2001},
	note = {Publisher: American Physical Society},
	pages = {014509},
	file = {APS Snapshot:/Users/gautam/Zotero/storage/65VM6HV9/PhysRevB.65.html:text/html;Full Text PDF:/Users/gautam/Zotero/storage/ATV94YLT/Halterman and Valls - 2001 - Proximity effects at ferromagnet-superconductor in.pdf:application/pdf}
}
