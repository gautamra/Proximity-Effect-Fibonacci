	pages = {46},
	file = {Jagannathan and Tarzia - 2020 - Re-entrance and localization phenomena in disorder.pdf:/Users/gautam/Zotero/storage/89M2RKQT/Jagannathan and Tarzia - 2020 - Re-entrance and localization phenomena in disorder.pdf:application/pdf}
